enum RequestCmdOffset
{
	REQUEST_INFO = 1,
	REQUEST_STATUS = 2,
	REQUEST_TASKCANCEL = 3,
	REQUEST_TASKSTATUS = 4,
	REQUEST_SETMODE = 5,
	REQUEST_ALARMCLEAR = 6,
	REQUEST_SYSRESET = 7,
	REQUEST_TASKPAUSE = 8,
	REQUEST_TASKRESUME = 9,
	REQUEST_SWEMS = 10,
	REQUEST_TIMESYNC = 11,
	REQUEST_READPARAM = 12,
	REQUEST_WRITEPARAM = 13,
	REQUEST_TASK = 20,
};

enum ResponseCmdOffset
{
	RESPONSE_INFO = 1,
	RESPONSE_STATUS = 2,
	RESPONSE_TASKCANCEL = 3,
	RESPONSE_TASKSTATUS = 4,
