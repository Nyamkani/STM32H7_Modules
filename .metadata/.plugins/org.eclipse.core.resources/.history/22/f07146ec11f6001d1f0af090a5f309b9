typedef struct
{
	uint16_t name_;
	uint16_t fw_version_;
}robot_info_;

typedef struct
{
	uint16_t configIP_ADDR0;
	uint16_t configIP_ADDR1;
	uint16_t configIP_ADDR2;
	uint16_t configIP_ADDR3;

	uint16_t configNET_MASK0;
	uint16_t configNET_MASK1;
	uint16_t configNET_MASK2;
	uint16_t configNET_MASK3;					
	
	uint16_t configGW_ADDR0;
	uint16_t configGW_ADDR1;
	uint16_t configGW_ADDR2;
	uint16_t configGW_ADDR3;
	
	
	
	
}ipaddress_info_;