enum RequestCmdOffset
{
	REQUEST_INFO = 1,
	REQUEST_STATUS = 2,
	REQUEST_TASKSTATUS = 3,
	REQUEST_SETMODE = 4,
	REQUEST_ALARMCLEAR = 5,
	REQUEST_SYSRESET = 6,
	REQUEST_TASKPAUSE = 7,
	REQUEST_TASKRESUME = 8,
	REQUEST_SWEMS = 9,
	REQUEST_TIMESYNC = 10,
	REQUEST_READPARAM = 11,
	REQUEST_WRITEPARAM = 12,
	REQUEST_TASK = 20,
};

enum AckCmdOffset
{
	ACK_TASK = 1,
	ACK_ALERT = 2,
};

enum ApproveCmdOffset
{
	APPROVE_ENTERELEVATOR = 1,
	APPROVE_LEAVEELEVATOR = 2,
	APPROVE_BUFFERPICK = 3,
	APPROVE_BUFFERPLACE = 4,
};

enum ExcuteCmdOffset
{
	EXCUTE_ENTERELEVATOR = 1,
	EXCUTE_LEAVEELEVATOR = 2,
	EXCUTE_BUFFERPICK = 3,
	EXCUTE_BUFFERPLACE = 4,
};

enum HeartbeatCmdOffset
{
	heartbeat = 1,
};


enum RecvCmdRange
{
	REQUEST_CMD_RANGE = 100,

	ACK_CMD_RANGE = 200,

	APRROVE_CMD_RANGE = 300,

	EXCUTE_CMD_RANGE = 400,

	HEARTTBEAT_CMD_RANGE = 900,

};


